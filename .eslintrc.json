{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],

  "rules": {
    "react/prop-types": 0,
    "no-console": 1,
    "no-unused-vars": 1,
    "for-direction": 0,
    "getter-return": 0,
    "import/default": 1
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "prettier",
    "eslint-plugin-prettier",
    "eslint-plugin-react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
